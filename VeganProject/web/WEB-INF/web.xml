<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>VeganProject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
   <servlet>
    <servlet-name>vgList</servlet-name>
    <servlet-class>com.a2bo.main.controller.MainController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>welcome</servlet-name>
    <servlet-class>com.a2bo.welcome.Welcome</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>member</servlet-name>
    <servlet-class>com.a2bo.member.controller.MemberController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>mypage</servlet-name>
    <servlet-class>com.a2bo.mypage.controller.MypageController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>calendar</servlet-name>
    <servlet-class>com.a2bo.calendar.controller.CalendarController</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>menuInfo</servlet-name>
    <servlet-class>com.a2bo.review.controller.RevContr</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>welcome</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>member</servlet-name>
    <url-pattern>/member/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>mypage</servlet-name>
    <url-pattern>/mypage/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>calendar</servlet-name>
    <url-pattern>/calendar/*</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>vgList</servlet-name>
  	<url-pattern>/main/*</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>menuInfo</servlet-name>
  	<url-pattern>/menuinfo/*</url-pattern>
  </servlet-mapping>
  
</web-app>